{
  "name": "income-expense-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "firebase emulators:exec --only auth,firestore --project=demo-project --ui 'ng serve'",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "firebase emulators:exec --only auth,firestore --project=demo-project 'ng test --detect-open-handles && npx jest --config  firebase-jest.config.js --detect-open-handles'",
    "test:frontend": "firebase emulators:exec --only auth,firestore --project=demo-project 'ng test --detect-open-handles --watch-all'",
    "test:rules": "firebase emulators:exec --only auth,firestore --project=demo-project 'npx jest --config  firebase-jest.config.js --detect-open-handles'",
    "lint": "ng lint",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.3",
    "@angular/common": "^17.3.3",
    "@angular/compiler": "^17.3.3",
    "@angular/core": "^17.3.3",
    "@angular/fire": "^7.6.1",
    "@angular/forms": "^17.3.3",
    "@angular/platform-browser": "^17.3.3",
    "@angular/platform-browser-dynamic": "^17.3.3",
    "@angular/router": "^17.3.3",
    "@ngrx/effects": "^17.1.1",
    "@ngrx/router-store": "^17.1.1",
    "@ngrx/store": "^17.1.1",
    "@ngrx/store-devtools": "^17.1.1",
    "bootstrap": "^5.3.0",
    "chart.js": "^4.3.0",
    "ng2-charts": "^4.1.1",
    "rxjs": "~7.8.0",
    "sweetalert2": "^11.7.12",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.3",
    "@angular-eslint/builder": "17.3.0",
    "@angular-eslint/eslint-plugin": "17.3.0",
    "@angular-eslint/eslint-plugin-template": "17.3.0",
    "@angular-eslint/schematics": "17.3.0",
    "@angular-eslint/template-parser": "17.3.0",
    "@angular/cli": "~17.3.3",
    "@angular/compiler-cli": "^17.3.3",
    "@firebase/rules-unit-testing": "^2.0.7",
    "@hirez_io/observer-spy": "^2.2.0",
    "@types/jest": "^29.5.2",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "canvas": "^2.11.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "firebase-admin": "^11.9.0",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "ng-mocks": "^14.11.0",
    "prettier": "2.8.8",
    "typescript": "~5.4.4"
  }
